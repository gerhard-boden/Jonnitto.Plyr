prototype(Plyr:CSS) <  prototype(TS:Tag) {
	@context.settings = ${Configuration.setting('Jonnitto.Plyr')}
	tagName = 'link'
	attributes {
		rel = 'stylesheet'
		href = TS:ResourceUri {
			path = 'resource://Jonnitto.Plyr/Public/Main.css'
		}
	}
	@position = 'end'

	@if.include = ${(settings.alwaysInclude || q(node).children(settings.selector.collection).find(settings.selector.content).is()) && settings.includeCSS}
}
